  "configurations": [
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/include/SDL2**",
        "${workspaceFolder}/include/**"
      ],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "compilerPath": "C:/MinGW/bin/g++.exe",
